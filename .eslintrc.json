{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/function-component-definition": 0,
    "react/jsx-filename-extension": 0,
    "react/self-closing-comp": 0,
    "react/forbid-prop-types": 0
  }
}
